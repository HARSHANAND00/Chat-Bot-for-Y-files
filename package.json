    "url": "https://github.com/HARSHANAND00/Chat-Bot-for-Y-files/issues"
  },
  "homepage": "https://github.com/HARSHANAND00/Chat-Bot-for-Y-files#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "zod": "^3.25.67",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "chmod-cli": "^2.0.1",
    "cross-env-shell": "^7.0.3",
    "typescript": "^5.8.3"
