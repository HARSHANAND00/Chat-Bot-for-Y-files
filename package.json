{
  "name": "mcp-documentation-server",
  "version": "1.0.0",
  "description": "MCP server for documentation retrieval and website scraping",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:stdio": "node dist/index.js --transport=stdio",
    "start:sse": "node dist/index.js --transport=sse --port=3100",
    "dev": "tsc && node dist/index.js",
    "dev:stdio": "tsc && node dist/index.js --transport=stdio",
    "dev:sse": "tsc && node dist/index.js --transport=sse --port=3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HARSHANAND00/Chat-Bot-for-Y-files.git"
  },
